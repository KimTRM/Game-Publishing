CREATE TABLE `mytabless` (
  `ID` varchar(4) NOT NULL,
  `GameTitle` varchar(50) DEFAULT NULL,
  `GameGenre` varchar(50) DEFAULT NULL,
  `ReleaseDate` varchar(100) NOT NULL,
  `GamePlayTime` smallint(6) DEFAULT NULL,
  `Horology_Time` varchar(8) DEFAULT NULL,
  `GameDescription` text DEFAULT NULL,
  `GameIcon` varchar(100) DEFAULT NULL,
  `GameFile` varchar(100) DEFAULT NULL,
  `FBLink` varchar(200) DEFAULT NULL,
  `YTLink` varchar(200) DEFAULT NULL,
  `InstaLink` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `ID` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci

 